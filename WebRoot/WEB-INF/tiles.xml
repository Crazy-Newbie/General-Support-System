<tiles-definitions>
	<definition name="mainLayout"
		template="/WEB-INF/jsp/templateV2/main_layout.jsp">
		<put-attribute name="title"
			value="/WEB-INF/jsp/templateV2/title.jsp" />
		<put-attribute name="navbar"
			value="/WEB-INF/jsp/templateV2/navbar.jsp" />
		<put-attribute name="sidebar"
			value="/WEB-INF/jsp/templateV2/sidebar.jsp" />
		<put-attribute name="message"
			value="/WEB-INF/jsp/templateV2/message.jsp" />
		<put-attribute name="body" value="" />
	</definition>
	<definition name="loginLayout"
		template="/WEB-INF/jsp/templateV2/login_layout.jsp">
